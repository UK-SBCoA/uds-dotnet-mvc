@model ParticipantDemographics
@{
}
<tr>
    <td><span class="counter"></span></td>
    <td>
        <div class="form-group">
            <label asp-for="HispanicLatinoEthnicity" class="control-label"></label>
            <div><span class="text-danger" asp-validation-for="HispanicLatinoEthnicity"></span></div>
        </div>
    </td>
    <td>

        <div class="form-check">
            <input class="form-check-input" asp-for="HispanicLatinoEthnicity" data-toggle-values="[0,9]" data-toggle-targets="HispanicLatinoOrigins" type="radio" id="HispanicLatinoEthnicity[1]" value="0" name="HispanicLatinoEthnicity">
            <label class="form-check-label" for="HispanicLatinoEthnicity[1]"> <span class="badge badge-pill badge-light">0</span> No  (If No, SKIP TO QUESTION 9)</label>
        </div>

        <div class="form-check">
            <input class="form-check-input" asp-for="HispanicLatinoEthnicity" data-toggle-values="[0,9]" data-toggle-targets="HispanicLatinoOrigins" type="radio" id="HispanicLatinoEthnicity[2]" value="1" name="HispanicLatinoEthnicity">
            <label class="form-check-label" for="HispanicLatinoEthnicity[2]"> <span class="badge badge-pill badge-light">1</span> Yes</label>
        </div>

        <div class="form-check">
            <input class="form-check-input" asp-for="HispanicLatinoEthnicity" data-toggle-values="[0,9]" data-toggle-targets="HispanicLatinoOrigins" type="radio" id="HispanicLatinoEthnicity[3]" value="9" name="HispanicLatinoEthnicity">
            <label class="form-check-label" for="HispanicLatinoEthnicity[3]"> <span class="badge badge-pill badge-light"> 9</span> Unknown (If Unknown, SKIP TO QUESTION 9)</label>
        </div>

    </td>
</tr>
