                <input type="text" class="form-control" id="HispanicLatinoOriginsOther" asp-for="HispanicLatinoOriginsOther" disabled>
            </div>
        </div>

        <div class="form-check">
            <input class="form-check-input" type="radio" asp-for="HispanicLatinoOrigins" id="HispanicLatinoOrigins[7]" value="99" name="HispanicLatinoOrigins" data-toggle-values="[1,2,3,4,5,6,99]" data-toggle-targets="HispanicLatinoOriginsOther">
            <label class="form-check-label" for="HispanicLatinoOrigins[7]"> <span class="badge badge-pill badge-light">99</span> Unknown</label>
