@model Symptoms
@using UDS.Net.Data.Dtos;

@{
    var title = "B9 Symptoms";
    var visitInfo = "";
    var isIVP = Equals(ViewBag.VisitType, VisitType.IVP);
    if (visitInfo == null)
    {
        visitInfo = " - " + Model.Visit.Participant.Id + " - Visit " + Model.Visit.VisitNumber;
    }
    ViewData["Title"] = title + visitInfo;
}

@await Component.InvokeAsync("VisitHeader", new { visitId = Model.Visit.Id, participationId = Model.Visit.Participant.Id, participantProfile = Model.Visit.Participant.Profile })
@await Component.InvokeAsync("FormHeader", new { currentVisit = Model.Visit, currentForm = Model, formTitle = title })

<div class="row">

    <div class="col-md-12 p-4">
        <p class="lead">INSTRUCTIONS: This form is to be completed by the clinician. For additional clarification and examples, see UDS Coding Guidebook for @Html.DisplayTextFor(v => v.Visit.VisitType) Visit Packet, Form B9. Check <u>only</u> one box per question.</p>
    </div>

    <div class="col-md-12 p-4">
        <form method="POST" asp-action="Edit" class="counterreset">
            <div class="row">
                <div class="col-md-12">
                    <div class="table-responsive">
                        <table class="table table-bordered">
                            <thead class="thead-light">
                                <tr>
                                    <th colspan="2">
                                        Declines in memory reported by subject and co-participant
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="w-50">
                                        1. <label asp-for="SubjectMemory"></label>
                                        <div><span class="text-danger" asp-validation-for="SubjectMemory"></span></div>
                                    </td>
                                    <td>
                                        <div>
                                            <input type="radio" asp-for="SubjectMemory" id="SubjectMemory[0]" value="0" /><label for="SubjectMemory[0]"><span class="badge badge-pill badge-light">0</span> No</label>
                                        </div>
                                        <div>
                                            <input type="radio" asp-for="SubjectMemory" id="SubjectMemory[1]" value="1" /><label for="SubjectMemory[1]"><span class="badge badge-pill badge-light">1</span> Yes</label>
                                        </div>
                                        <div>
                                            <input type="radio" asp-for="SubjectMemory" id="SubjectMemory[8]" value="8" /><label for="SubjectMemory[8]"><span class="badge badge-pill badge-light">8</span> Could not be assessed/subject is too impaired</label>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        2. <label asp-for="CoParticipantMemory"></label>
                                        <div><span class="text-danger" asp-validation-for="CoParticipantMemory"></span></div>
                                    </td>
                                    <td>
                                        <div>
                                            <input type="radio" asp-for="CoParticipantMemory" id="CoParticipantMemory[0]" value="0" /><label for="CoParticipantMemory[0]"><span class="badge badge-pill badge-light">0</span> No</label>
                                        </div>
                                        <div>
                                            <input type="radio" asp-for="CoParticipantMemory" id="CoParticipantMemory[1]" value="1" /><label for="CoParticipantMemory[1]"><span class="badge badge-pill badge-light">1</span> Yes</label>
                                        </div>
                                        <div>
                                            <input type="radio" asp-for="CoParticipantMemory" id="CoParticipantMemory[8]" value="8" /><label for="CoParticipantMemory[8]"><span class="badge badge-pill badge-light">8</span> There is no co-participant</label>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="table-responsive">
                        <table class="table table-bordered">
                            <thead class="thead-light">
                                <tr>
                                    <th colspan="4">
                                        Cognitive symptoms
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td colspan="1" class="w-50">
