                                        <input type="radio" asp-for="VisuospatialFunction" id="VisuospatialFunction[0]" value="0" /><label for="VisuospatialFunction[0]"><span class="badge badge-pill badge-light">0</span> No</label>
                                    </td>
                                    <td>
                                        <input type="radio" asp-for="VisuospatialFunction" id="VisuospatialFunction[1]" value="1" /><label for="VisuospatialFunction[1]"><span class="badge badge-pill badge-light">1</span> Yes</label>
                                    </td>
                                    <td>
                                        <input type="radio" asp-for="VisuospatialFunction" id="VisuospatialFunction[9]" value="9" /><label for="VisuospatialFunction[9]"><span class="badge badge-pill badge-light">9</span> Unknown</label>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        4f. <b>Attention, concentration</b> <label asp-for="AttentionConcentration"></label>
                                        <div><span class="text-danger" asp-validation-for="AttentionConcentration"></span></div>
                                    </td>
                                    <td>
