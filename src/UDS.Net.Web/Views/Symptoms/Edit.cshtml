                                    <td>
                                        <input type="radio" asp-for="AttentionConcentration" id="AttentionConcentration[9]" value="9" /><label for="AttentionConcentration[9]"><span class="badge badge-pill badge-light">9</span> Unknown</label>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        4g. <b>Fluctuating cognition</b> <label asp-for="FluctuatingCognition"></label>
                                        <div><span class="text-danger" asp-validation-for="FluctuatingCognition"></span></div>
                                    </td>
                                    <td>
                                        <input type="radio" asp-for="FluctuatingCognition" id="FluctuatingCognition[0]" value="0" data-toggle-targets="FluctuatingCognitionAge" data-toggle-values="[0,9]"><label for="FluctuatingCognition[0]"><span class="badge badge-pill badge-light">0</span> No</label>
                                    </td>
                                    <td>
                                        <input type="radio" asp-for="FluctuatingCognition" id="FluctuatingCognition[1]" value="1" data-toggle-targets="FluctuatingCognitionAge" data-toggle-values="[0,9]"><label for="FluctuatingCognition[1]"><span class="badge badge-pill badge-light">1</span> Yes</label>
