@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script>
        $(() => {        
            /*
                -- Subject has abnormal findings 
                1.No findings end the form
                2.Yes continues the form
                3.Yes w/ Age Skip to question 8
                - Casey Spires
            */
            var formAbnormal = $('input[name=AbnormalNeurologicalExamFindings]:checked');
            if(formAbnormal.length > 0) {
                var examResultValue = formAbnormal.first().val();
                notAbnormalDisable(examResultValue);
            }
            $('input[name=AbnormalNeurologicalExamFindings]').on('change', (event) => {
                var examResultValue = $(event.target).val();
                notAbnormalDisable(examResultValue);
            });
            /* Question 2 */
            var parkinsonian = $('input[name=ParkinsonianSigns]:checked');
            if(parkinsonian.val() == "false" && parkinsonian.is(':checked'))
            {
                toggleChildren('Parkinsonian', true);
            } 
            if(parkinsonian.val() == "true" && parkinsonian.is(':checked')) {
                toggleChildren('Parkinsonian', false);
            }
            $('input[name=ParkinsonianSigns]').on('change', (event) => {
                if($(event.target).val() == "false" && $(event.target).is(':checked'))
                {
                    toggleChildren('Parkinsonian', true);
                } 
                if($(event.target).val() == "true" && $(event.target).is(':checked')) {
                    toggleChildren('Parkinsonian', false);
                }
             });
             /* Question 3 */
            var cortical = $('input[name=CerebrovascularDiseaseSigns]:checked');
            if(cortical.val() == "false" && cortical.is(':checked'))
            {
                toggleChildren('Cortical', true);
            }
            if(cortical.val() == "true" && cortical.is(':checked')) {
                toggleChildren('Cortical', false);
            }
            $('input[name=CerebrovascularDiseaseSigns]').on('change', (event) => {
                if($(event.target).val() == "false" && $(event.target).is(':checked'))
                {
                    toggleChildren('Cortical', true);
                } 
                if($(event.target).val() == "true" && $(event.target).is(':checked')) {
                    toggleChildren('Cortical', false);
                }
            });
            /* Question 5 */
            var psp = $('input[name=PSP]:checked').first();
            if(psp.val() == "false" && psp.is(':checked'))
            {
                toggleChildren('PSP_', true);
                toggleChildren('CBS_', true);
            } 
            if(psp.val() == "true" && psp.is(':checked')) {
                toggleChildren('PSP_', false);
                toggleChildren('CBS_', false)
            }
             $('input[name=PSP]').on('change', (event) => {
                if($(event.target).val() == "false" && $(event.target).is(':checked'))
                {
                    toggleChildren('PSP_', true);
                    toggleChildren('CBS_', true);
                } 
                if($(event.target).val() == "true" && $(event.target).is(':checked')){
                    toggleChildren('PSP_', false);
                    toggleChildren('CBS_', false)
                }
             });
        });
        function triggerChange() {
            $('input[name=ParkinsonianSigns]').triggerHandler('change');
            $('input[name=CerebrovascularDiseaseSigns]').triggerHandler('change');
            $('input[name=PSP]').triggerHandler('change');
        }
        function notAbnormalDisable(examResultValue) {
            /* Disables Everything*/
            if(examResultValue == 0 || examResultValue == 2) {
                $('input:not([data-always-enabled])').each((index, elem) => {
                    const jInput = $(elem);
                    jInput.prop('disabled', true);
                    if(jInput.attr('name') == 'OtherFindings' && examResultValue == 2) {
                        jInput.prop('disabled', false);
                        const otherFindingsSpecify = $('input[name=OtherFindingsSpeicify]')
                        if(jInput.val() == 1) {
                            otherFindingsSpecify.prop('disabled', false);
                        } else {
                            otherFindingsSpecify.prop('disabled', true);
                        }
                    }
                    if(jInput.attr('name') == '__RequestVerificationToken') {
                        jInput.prop('disabled', false);
                    }
                });
            }
            /* Enable entire form */
            if(examResultValue == 1) {
                $('input:not([data-always-enabled])').each((index, elem) => {
                    var jInput = $(elem);
                    jInput.prop('disabled', false);
                    
                });
            }  
            triggerChange();
        }
        function toggleChildren(controlPrefix, toggleState) {
            var controls = $(`input[name^=${controlPrefix}]:not([data-parent])`);
            controls.each((index, elem) => {
                $(elem).prop('disabled', toggleState)
            });
            
        }   
    </script>
    <script src="~/js/RadioFieldToggle.js"></script>
}