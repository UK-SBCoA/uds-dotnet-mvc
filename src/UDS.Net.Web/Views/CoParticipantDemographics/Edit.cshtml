                                <div><span asp-validation-for="BirthMonth" class="text-danger"></span></div>
                                <div><span asp-validation-for="BirthDay" class="text-danger" type="number"></span></div>
                                <div><span asp-validation-for="BirthYear" class="text-danger" type="number"></span></div>
                            </div>
                        </td>
                        <td class="colspan="1">
                            <input asp-for="BirthMonth" class="form-control" type="number" />
                        </td>
                        <td class="colspan="1">
                            <input asp-for="BirthDay" class="form-control" type="number" />
                        </td>
                        <td class="colspan="1">
                            <input asp-for="BirthYear" class="form-control" type="number" />
                        </td>
                    </tr>
                    <tr>
