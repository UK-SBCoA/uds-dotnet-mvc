                            <div class="form-check">
                                <input class="form-check-input" type="radio" asp-for="EthnicOrigins" value="99" name="EthnicOrigins" id="EthnicOrigins[8]" data-toggle-values="[1,2,3,4,5,6,99]" data-toggle-targets="EthnicOriginsOther" />
